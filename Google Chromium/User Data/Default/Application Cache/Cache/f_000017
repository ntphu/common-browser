		<script type="text/javascript">
	function ie_placeholder() {
		if (!Modernizr.input.placeholder) {
			$('body :input[placeholder]:not([type="password"])').each(function() {
				$(this).on({
					'focus': function() {
						if ($(this).val() == $(this).attr('placeholder')) {
							$(this).val('');
						}
					},
					'blur': function() {
						if ($(this).val() == '' || $(this).val() == $(this).attr('placeholder')) {
							$(this).val($(this).attr('placeholder'));
						}
					}
				}).trigger('blur');
			});
		}
	}
			Events.subscribe(Events.navigation.page_loaded, ie_placeholder);
	</script><div class="modal" id="modal" role="dialog" style="display: none;">
